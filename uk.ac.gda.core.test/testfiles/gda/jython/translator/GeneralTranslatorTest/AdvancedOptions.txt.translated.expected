def qbpmAverage(qbpm):
	f=open("/home/" + Beamline + "user/scripts/qbpms.txt","r")
	total=0.0
	array=[]
	for line in f:
		if qbpm in line:
			array.extend([line[7:12]])
	for i in array:
		total += float(i)
	average = total / len(array)
	f.close()
	return str(round(average,4))

