<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension point="org.eclipse.ui.editors">
    <editor class="uk.ac.gda.bimorph.BimorphParametersEditor"
            default="true"
            id="gda.org.myls.bimorph.BimorphParametersEditor"
            name="BimorphParametersEditor">
         <contentTypeBinding
               contentTypeId="org.myls.bimorph.BimorphParametersEditor">
         </contentTypeBinding>            
     </editor>
  </extension>
  <extension point="org.eclipse.core.contenttype.contentTypes">
     <content-type
       base-type="org.eclipse.core.runtime.xml"
       describer="uk.ac.gda.bimorph.BimorphParametersDescriber"
       file-extensions="xml"
       id="org.myls.bimorph.BimorphParametersEditor"
       name="BimorphParameters File Content"
       priority="high">
     </content-type>
    
  </extension>
   <extension point="uk.ac.common.beans.factory">
      <bean
            class="uk.ac.gda.bimorph.BimorphParameters"
            describer="uk.ac.gda.bimorph.BimorphParametersDescriber">
      </bean>
      
  </extension>
   <extension
         point="uk.ac.diamond.daq.jython.api.scriptLocations">
      <location
            folder="uk.ac.gda.bimorph/scripts">
      </location>
   </extension>
</plugin>
