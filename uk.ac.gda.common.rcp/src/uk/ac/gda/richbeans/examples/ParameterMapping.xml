<?xml version="1.0" encoding="UTF-8"?>
<mapping>
	
	<class name="uk.ac.gda.richbeans.examples.ExampleParameters">
		<map-to xml="ExampleParameters" />
		<field name="element" type="string">
			<bind-xml name="element" node="element" />
		</field>
		<field name="edge" type="string">
			<bind-xml name="edge" node="element" />
		</field>
		<field name="start" type="double">
			<bind-xml name="start" node="element" />
		</field>
		<field name="stop" type="double">
			<bind-xml name="stop" node="element" />
		</field>
		<field name="items" type="uk.ac.gda.richbeans.examples.ExampleItem"
			collection="collection" set-method="addItem" get-method="getItems">
			<bind-xml name="items" node="element"/>
		</field>
	</class>
	
	<class name="uk.ac.gda.richbeans.examples.ExampleItem">
		<map-to xml="ExampleItem" />
		<field name="x" type="double">
			<bind-xml name="x" node="element" />
		</field>
		<field name="y" type="double">
			<bind-xml name="y" node="element" />
		</field>
	</class>
	
</mapping>
	